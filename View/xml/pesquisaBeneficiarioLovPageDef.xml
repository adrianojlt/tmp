        <Item Value="Niss"/>
        <Item Value="Bi"/>
        <Item Value="Numbenef"/>
        <Item Value="NomeNormalizado"/>
        <Item Value="ObitoId"/>
      </AttrNames>
        </table>
        <table IterBinding="InformacaoUtenteIterator" id="InformacaoUtente">
          <AttrNames>
            <Item Value="Idutente"/>
            <Item Value="Datanascimento"/>
            <Item Value="Entresp"/>
            <Item Value="Nomeutente"/>
